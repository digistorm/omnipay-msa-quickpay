{
    "name": "digistorm/omnipay-msa-quickpay",
    "type": "library",
    "description": "MSA (My Student Account) QuickPay API driver for the Omnipay payment processing library",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "msa",
        "mystudentaccount",
        "quickpay"
    ],
    "homepage": "https://github.com/digistorm/omnipay-msa-quickpay",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Omnipay\\MsaQuickpay\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "omnipay/common": "^3",
        "symfony/http-client": "^7.1"
    },
    "require-dev": {
        "omnipay/tests": "^4",
        "http-interop/http-factory-guzzle": "^1.2",
        "rector/rector": "^1.2",
        "phpstan/phpstan": "^1.12",
        "friendsofphp/php-cs-fixer": "^3.64",
        "spaze/phpstan-disallowed-calls": "^3.5"
    },
    "config": {
        "platform": {
            "php": "8.2"
        },
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "prefer-stable": true
}
